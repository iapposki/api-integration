// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./sqlite.db"
}

model ProductID {
  id        Int @id @default(autoincrement())
  productID Int @unique
}

model RawData {
  id              Int    @id @default(autoincrement())
  startDate       String
  startTime       String
  endTime         String
  providerSlotId  String @unique
  remaining       Int
  currencyCode    String
  variantId       Int
  paxAvailability Pax[]
}

model Pax {
  id            Int      @id @default(autoincrement())
  max           Int
  min           Int
  remaining     Int
  type          String
  isPrimary     Boolean?
  description   String
  name          String
  discount      Float
  finalPrice    Float
  originalPrice Float
  currencyCode  String
  rawData       RawData? @relation(fields: [aPIdataId], references: [id])
  aPIdataId     Int?
}
